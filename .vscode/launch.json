{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Launch no args",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/main.go",
            "showLog": true
        }, 
        {
            "name": "Launch LIB no args",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/main.go",
            "env": {},
            "args": ["lib"],
            "showLog": true
        },
        {
            "name": "Launch LIB LIST JSON",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/main.go",
            "env": {},
            "args": ["lib", "list", "--format", "json"],
            "showLog": true
        },
        {
            "name": "Launch LIB LIST UPDATE",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/main.go",
            "env": {},
            "args": ["lib", "--update-index", "-v"],
            "showLog": true
        },
        {
            "name": "Launch LIB INSTALL JSON",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/main.go",
            "env": {},
            "args": ["lib", "install", "YoutubeApi", "--format", "json"],
            "showLog": true
        },
        {
            "name": "Launch LIB UNINSTALL JSON",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/main.go",
            "env": {},
            "args": ["lib", "uninstall", "YoutubeApi", "--format", "json"],
            "showLog": true
        },
        {
            "name": "Launch LIB DOWNLOAD",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/main.go",
            "env": {},
            "args": ["lib", "download", "Zumo32U4", "YoutubeApi", "YouMadeIt", "XLR8SPI", "XLR8Servo", "XLR8Pong", "XLR8NeoPixel"],
            "showLog": true
        },
        {
            "name": "Launch CORE LIST JSON",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/main.go",
            "env": {},
            "args": ["core", "list", "--format", "json"],
            "showLog": true
        },
        {
            "name": "Launch CORE DOWNLOAD",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/main.go",
            "env": {},
            "args": ["core", "download", "arduino:samd"],
            "showLog": true
        },
    ]
}